// Test 07: Enums and match expressions
enum Color:
    Red
    Green
    Blue
    Custom(Int, Int, Int)

enum Result:
    Ok(Int)
    Err(String)

fn color_name(c: Color) -> String:
    match c:
        Red => return "red"
        Green => return "green"
        Blue => return "blue"
        Custom(r, g, b) => return "custom"
    return "unknown"

fn main():
    let c1 = Color::Red
    let c2 = Color::Green
    let c3 = Color::Custom(255, 128, 0)
    
    println("c1 is " + color_name(c1))
    println("c2 is " + color_name(c2))
    println("c3 is " + color_name(c3))
    
    // Result pattern
    let success = Result::Ok(42)
    let failure = Result::Err("something went wrong")
    
    match success:
        Ok(val) => println("Success with value: " + str(val))
        Err(msg) => println("Error: " + msg)
    
    match failure:
        Ok(val) => println("Success with value: " + str(val))
        Err(msg) => println("Error: " + msg)
